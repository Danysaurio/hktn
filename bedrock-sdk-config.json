      },
      "input": {
        "type": "structure",
        "required": ["modelId", "input"],
        "members": {
          "modelId": {
            "location": "uri",
            "locationName": "modelId"
          },
          "input": {
            "location": "body"
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "result": {}
        }
      }
    }
  }
}